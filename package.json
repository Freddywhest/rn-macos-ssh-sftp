{
  "name": "rn-macos-ssh-sftp",
  "version": "0.1.1",
  "description": "SSH and SFTP client library for React Native on macOS",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "react-native": "lib/index.js",
  "homepage": "https://github.com/FreddyWhest/rn-macos-ssh-sftp#readme",
  "files": [
    "lib",
    "macos",
    "*.podspec",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "_postinstall": "npm run compile",
    "prepare": "husky",
    "build": "npm run compile && npm pack",
    "precompile": "npm run lint:build",
    "compile": "npx tsc",
    "lint": "eslint .",
    "lint:build": "cross-env NODE_ENV=production npm run lint -- --max-warnings 20"
  },
  "keywords": [
    "react-native",
    "macos",
    "ssh",
    "sftp",
    "file-transfer",
    "remote-access"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FreddyWhest/rn-macos-ssh-sftp.git"
  },
  "author": "Alfred Nti <alfrednti5000@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-macos": "*"
  },
  "devDependencies": {
    "@types/react": "^19.1.03",
    "typescript": "^5.8.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "react": "19.1.1",
    "react-native": "0.82.0"
  }
}
